    "marked": "^5.0.2",
    "next": "13.4.1",
    "next-auth": "^4.22.1",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
